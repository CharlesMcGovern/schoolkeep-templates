<div class="cover-virtual-event">
  <span class="uk-float-right">
    {{ session.join_link }}
  </span>
  <span class="uk-text-muted">
    {{ l(session.starts_at, 'day_month_long') }} |
    {{ l(session.starts_at, 'clock_with_zone') }} -
    {{ l(session.ends_at, 'clock_with_zone') }}
  </span>
  <h4 class="uk-comment-title">
    <img src="{{ session.provider_logo_url }}" class="provider-logo"\>
    {{ session.virtual_event_name }}
  </h4>
</div>
<hr>
